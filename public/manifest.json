{
  "manifest_version": 2,
  "name": "React Chrome Extension",
  "version": "0.1.0",
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "browser_action": {
    "default_icon": {
      "192": "logo192.png",
      "512": "logo512.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "index.html",
    "inject.js",
    "static/*"
  ]
}
